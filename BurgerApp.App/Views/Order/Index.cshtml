@model List<OrderListViewModel>

<h2>List of orders:</h2>
<br />
<div class="card-container">
    @foreach (var order in @Model)
    {
        <partial name="~/Views/Shared/Partials/_OrderCardPartial.cshtml" for="@order" />
    }
</div>
<br />
<a class="btn btn-warning" asp-area="" asp-controller="Home" asp-action="Index">Home page</a>
<a class="btn btn-warning" asp-area="" asp-controller="Order" asp-action="Create">Create new order</a>